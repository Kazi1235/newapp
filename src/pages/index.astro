<script>
  // Table of content
  // h2 will be converted to id
  let tocs = document.querySelectorAll("h2");
  let listToc = document.querySelector(".list-toc");
  for (var toc of tocs) {
    let str = toc.textContent;
    let idValue = str.replace(/\s/g, "-");
    toc.setAttribute("id", idValue);
    listToc.innerHTML += `<li><a  href="#${idValue}">${str}</a></li>`;
  }
  let listTocTarget = document.querySelectorAll(".list-toc a");
  listTocTarget.forEach((el) => {
    el.addEventListener("click", function () {
      for (var i = 0; i < listTocTarget.length; i++) {
        (listTocTarget[i].parentNode as HTMLElement).classList.add("active");
      }
      this.parentNode.classList.add("active");
    });
  });

